create table Client (
codeC char(4),
nom varchar(25),
prenom varchar(25),
age int,
permis varchar(10),
adresse varchar(50),
ville varchar(30),
PRIMARY KEY (codeC)
);

SELECT modele, COUNT(numLoc) AS nb_locations FROM VOITURE 
JOIN LOCATION ON voiture.immat = location.immat
GROUP BY modele HAVING COUNT(numLoc) >= 3;
