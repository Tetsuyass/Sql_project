
create database AgenceLocation;
use AgenceLocation;

create table Voiture (
immat char(6) primary key,
modele varchar(25),
marque varchar(25),
categorie varchar(25),
couleur varchar(25),
places tinyint,
achatA year,
compteur int,
prixJ float,
codeP char(3)
);

create table Propritaire (
codeP char(3),
pseudo varchar(25),
email varchar(50),
ville varchar(25),
annee year
);
