CREATE TABLE Location (
	codeC varchar(5) UNIQUE REFERENCES Client(codeC),
    immat char(6) UNIQUE REFERENCES Voiture(immat),
    annee year PRIMARY KEY,
    mois tinyint PRIMARY KEY,
    numLoc varchar(6),
    km smallint,
    duree tinyint,
    villeD varchar(25),
    villeA varchar(25),
    dateD date,
    dateA date
);
